{
  "name": "@mojaloop/bulk-api-adapter",
  "version": "12.1.0",
  "description": "Mojaloop Bulk API Adapter",
  "license": "Apache-2.0",
  "author": "ModusBox",
  "contributors": [
    "Georgi Georgiev <georgi.georgiev@modusbox.com>",
    "Miguel de Barros <miguel.debarros@modusbox.com>",
    "Steven Oderayi <steven.oderayi@modusbox.com>",
    "Valentin Genev <valentin.genev@modusbox.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mojaloop/bulk-api-adapter.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@hapi/basic": "6.0.0",
    "@hapi/good": "9.0.1",
    "@hapi/hapi": "20.2.1",
    "@hapi/inert": "6.0.5",
    "@hapi/vision": "6.1.0",
    "@mojaloop/object-store-lib": "11.1.2",
    "@mojaloop/central-services-error-handling": "11.3.0",
    "@mojaloop/central-services-health": "13.0.0",
    "@mojaloop/central-services-logger": "10.6.2",
    "@mojaloop/central-services-metrics": "11.0.0",
    "@mojaloop/central-services-shared": "16.0.0",
    "@mojaloop/central-services-stream": "10.7.0",
    "@mojaloop/event-sdk": "10.7.1",
    "@now-ims/hapi-now-auth": "2.1.0",
    "axios": "0.26.0",
    "blipp": "4.0.2",
    "boom": "7.3.0",
    "catbox": "10.0.6",
    "catbox-memory": "4.0.1",
    "commander": "9.0.0",
    "hapi-auth-bearer-token": "8.0.0",
    "hapi-openapi": "3.0.0",
    "hapi-swagger": "14.2.5",
    "immutable": "4.0.0",
    "joi": "^17.6.0",
    "mustache": "4.2.0",
    "parse-strings-in-object": "2.0.0",
    "rc": "1.2.8",
    "request": "2.88.2",
    "run-s": "0.0.0",
    "uuid4": "2.0.2"
  },
  "devDependencies": {
    "istanbul": "1.1.0-alpha.1",
    "nodemon": "2.0.15",
    "npm-audit-resolver": "2.3.1",
    "npm-check-updates": "12.5.0",
    "npm-run-all": "4.1.5",
    "pre-commit": "1.2.2",
    "proxyquire": "2.1.3",
    "rewire": "6.0.0",
    "sinon": "13.0.1",
    "standard": "16.0.4",
    "standard-version": "^9.3.2",
    "swagmock": "1.0.0",
    "tap-spec": "^5.0.0",
    "tap-xunit": "2.4.1",
    "tape": "5.5.2",
    "tapes": "4.1.0"
  },
  "pre-commit": [
    "lint",
    "dep:check",
    "test"
  ],
  "scripts": {
    "start": "run-p start:api",
    "start:api": "node src/api/index.js",
    "watch:api": "nodemon src/api/index.js",
    "regenerate": "yo swaggerize:test --framework hapi --apiPath './src/interface/swagger.yaml'",
    "standard": "standard",
    "standard:fix": "standard --fix",
    "lint": "npm run standard",
    "lint:fix": "npm run standard:fix",
    "test": "npm run test:unit",
    "test:all": "run-s test",
    "test:unit": "tape 'test/unit/**/*.test.js' | tap-spec",
    "test:xunit": "tape 'test/unit/**/*.test.js' | tap-xunit",
    "test:coverage": "istanbul cover tape -- 'test/unit/**/*.test.js'",
    "test:coverage-check": "npm run test:coverage && istanbul check-coverage",
    "audit:resolve": "SHELL=sh resolve-audit --production",
    "audit:check": "SHELL=sh check-audit --production",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u",
    "release": "standard-version --releaseCommitMessageFormat 'chore(release): {{currentTag}} [skip ci]'"
  },
  "generator-swaggerize": {
    "version": "4.1.0"
  },
  "main": "./src/api/index.js"
}
